#include <iostream>
#include <cmath>
using namespace std;

int TinhTong(int a[], int n)
{
    int sum = 0;
    for (int i = 0; i < n; i++){
        if (a[i] < a[i+1])
            sum += a[i+1];
    }
    return sum;
}
int main()
{   
    int a[1000];
    int n;
    cout << "Nhap so phan tu: ";
    cin >> n;
    for (int i = 0; i < n ; i++){
        cout << "Nhap phan tu thu " << i+1 << ": ";
        cin >> a[i];
    }
    cout << TinhTong(a, n);
    return 0;
}
