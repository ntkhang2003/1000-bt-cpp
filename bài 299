#include <iostream>
#include <cmath>
using namespace std;
void nhap(float *a, int &n)
{
    for (int i = 0; i < n ; i++)
    {
        cout << "Nhap phan tu thu " << i+1 << ": ";
        cin >> a[i];
    }
}
void xuat(float *a, int n)
{
    for (int i = 0; i < n; i++)
    {
        cout << a[i] << " ";
    }
}
int KiemTraTangDan(float *b, int nb)
{
    for(int i = 0; i < nb - 1; i++)
    {
        if(b[i] > b[i + 1])
        {
            return 0;
        }
    }
    return 1;
}
int KiemTraDuong(float *b, int nb)
{
    for (int i = 0; i < nb; i ++)
    {
        if (b[i] <= 0)
        {
            return 0;
        }
    }
    return 1;
}
void XuatMangCon(float *b, int nb)
{
    for(int i = 0; i < nb; i++)
    {
        cout << b[i] << " ";
    }
    cout << endl;
}
void LietKeDayConToanDuong(float *a, int n)
{
    int ChieuDai, count = 0;
    float b[1000];
    int nb;
    for(int i = 0; i < n; i++)
    {
        for(ChieuDai = i+1; ChieuDai < n; ChieuDai++)
        {
            nb = 0;
            for(int j = i; j <= ChieuDai; j++)
            {
                b[nb] = a[j];
                nb++;
            }
            if(KiemTraTangDan(b, nb) == 1 && KiemTraDuong(b, nb))
            {
                XuatMangCon(b, nb); 
            }
        }
    }
}
int main()
{   
    int n;
    cout << "Nhap so phan tu: ";
    cin >> n;
    float a[n];
    nhap(a, n);
    LietKeDayConToanDuong(a, n);
    return 0;
}
